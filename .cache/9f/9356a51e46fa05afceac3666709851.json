s {\r\n    background-color: $greyish-blue-light;\r\n    border-radius: 0.5rem;\r\n    padding: 1.8rem 3.6rem;\r\n    text-align: center;\r\n  }\r\n\r\n  &__facebook,\r\n  &__twitter,\r\n  &__youtube {\r\n    border-top: 4px solid;\r\n  }\r\n\r\n  &__facebook {\r\n    border-color: $fb-color;\r\n  }\r\n\r\n  &__twitter {\r\n    border-color: $twitter-color;\r\n  }\r\n\r\n  &__instagram {\r\n    position: relative;\r\n  }\r\n\r\n  &__instagram::before {\r\n    content: \"\";\r\n    position: absolute;\r\n    inset: 0;\r\n    padding-top: 0.4rem;\r\n    border-radius: 0.5rem;\r\n    background-image: linear-gradient(\r\n      to right,\r\n      hsl(37, 97%, 70%),\r\n      hsl(329, 70%, 58%)\r\n    );\r\n    -webkit-mask: linear-gradient(#fff 0 0) content-box,\r\n      linear-gradient(#fff 0 0);\r\n    -webkit-mask-composite: xor;\r\n    mask-composite: subtract;\r\n  }\r\n\r\n  &__youtube {\r\n    border-color: $youtube-color;\r\n  }\r\n\r\n  &__id-box {\r\n    @include flex(row, center, center, 0.8rem);\r\n    margin-bottom: 1.8rem;\r\n  }\r\n\r\n  &__id-name {\r\n    color: $greyish-blue-dark;\r\n    font-weight: 700;\r\n  }\r\n\r\n  &__no-of-followers {\r\n    font-size: 4.8rem;\r\n    font-weight: 700;\r\n  }\r\n\r\n  &__followers-text {\r\n    color: $greyish-blue-dark;\r\n    text-transform: uppercase;\r\n    font-size: 1.2rem;\r\n    letter-spacing: 4px;\r\n    margin-bottom: 2.4rem;\r\n  }\r\n\r\n  &__increase-box,\r\n  &__decrease-box {\r\n    @include flex(row, center, center, 0.4rem);\r\n    font-weight: 700;\r\n  }\r\n\r\n  &__increase-box {\r\n    color: $lime-green;\r\n  }\r\n\r\n  &__decrease-box {\r\n    color: $bright-red;\r\n  }\r\n\r\n  &__overview-heading {\r\n    font-size: 2.4rem;\r\n    color: $greyish-blue-dark;\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  &__overview-grid {\r\n    @include grid(4, 2.4rem, 1.8rem);\r\n  }\r\n\r\n  &__reach-box {\r\n    background-color: $greyish-blue-light;\r\n    border-radius: 0.5rem;\r\n    padding: 1.8rem;\r\n  }\r\n\r\n  &__reach-heading-box {\r\n    @include flex(row, space-between, center, 0);\r\n    margin-bottom: 2.4rem;\r\n  }\r\n\r\n  &__mode-of-reach {\r\n    font-size: 1.2rem;\r\n    font-weight: 700;\r\n    color: $greyish-blue-dark;\r\n  }\r\n\r\n  &__reach-description-box {\r\n    @include flex(row, space-between, center, 0);\r\n  }\r\n  &__total-reach {\r\n    font-size: 3rem;\r\n    font-weight: 700;\r\n  }\r\n}\r\n","sass\\abstract\\_mixins.scss":"@mixin flex($direction, $justify-content, $align-items, $gap) {\r\n  display: flex;\r\n  flex-direction: $direction;\r\n  justify-content: $justify-content;\r\n  align-items: $align-items;\r\n  gap: $gap;\r\n}\r\n\r\n@mixin grid($numOfCol, $col-gap, $row-gap) {\r\n  display: grid;\r\n  grid-template-columns: repeat($numOfCol, 1fr);\r\n  column-gap: $col-gap;\r\n  row-gap: $row-gap;\r\n}\r\n","sass\\abstract\\_variables.scss":"$lime-green: hsl(163, 72%, 41%);\r\n$bright-red: hsl(356, 69%, 56%);\r\n$fb-color: hsl(208, 92%, 53%);\r\n$twitter-color: hsl(203, 89%, 53%);\r\n$youtube-color: hsl(348, 97%, 39%);\r\n\r\n//Light Theme Colors\r\n$white: hsl(0, 0%, 100%);\r\n$greyish-blue-light: hsl(227, 47%, 96%);\r\n$greyish-blue-dark: hsl(228, 12%, 44%);\r\n$pale-blue: hsl(225, 100%, 98%);\r\n$dark-blue: hsl(230, 17%, 14%);\r\n"},"lineCount":null}},"error":null,"hash":"881a0b39accd1c36ef5840088344c1f7","cacheData":{"env":{}}}