nhtml {\r\n  font-size: 62.5%;\r\n}\r\n\r\nbody {\r\n  background: linear-gradient(\r\n      0deg,\r\n      var(--clr-bg) 70%,\r\n      var(--clg-top-pattern) 30%\r\n    )\r\n    no-repeat;\r\n  box-sizing: border-box;\r\n  height: 100vh;\r\n  font-family: \"Inter\", sans-serif;\r\n}\r\n\r\n.container-120w {\r\n  max-width: 120rem;\r\n  margin: 0 auto;\r\n}\r\n","sass\\main.scss":"@use \"abstract\" as *;\r\n@use \"base\" as *;\r\n\r\n.smd {\r\n  padding: 3.2rem 2.4rem;\r\n\r\n  &__heading-box {\r\n    @include flex(row, space-between, center, 0);\r\n    margin-bottom: 3.2rem;\r\n  }\r\n\r\n  &__heading,\r\n  &__no-of-followers,\r\n  &__total-reach {\r\n    color: var(--text-dark);\r\n  }\r\n\r\n  &__heading {\r\n    font-size: 3rem;\r\n  }\r\n\r\n  &__total-followers {\r\n    color: var(--text-light);\r\n    font-weight: 700;\r\n    font-size: 1.2rem;\r\n  }\r\n\r\n  &__mode {\r\n    @include flex(row, center, center, 1.2rem);\r\n\r\n    &--name {\r\n      color: var(--clr-toggle);\r\n      font-size: 1.2rem;\r\n      font-weight: 700;\r\n    }\r\n\r\n    &--dark {\r\n      background-color: $toggle-color;\r\n      border-radius: 3.2rem;\r\n      cursor: pointer;\r\n      display: block;\r\n      position: relative;\r\n      width: 4.8rem;\r\n      height: 2.4rem;\r\n      transition: all 0.3s ease;\r\n    }\r\n\r\n    &--dark.toggle-active {\r\n      background-image: linear-gradient(\r\n        to right,\r\n        hsl(210, 78%, 56%),\r\n        hsl(146, 68%, 55%)\r\n      );\r\n    }\r\n\r\n    &--dark.toggle-active &--slider {\r\n      left: 0;\r\n    }\r\n\r\n    &--slider {\r\n      position: absolute;\r\n      top: 0;\r\n      left: 2.4rem;\r\n      width: 2.4rem;\r\n      height: 2.4rem;\r\n      border-radius: 50%;\r\n      background-color: var(--clg-top-pattern);\r\n      transform: scale(0.8);\r\n      transition: all 0.3s ease;\r\n    }\r\n  }\r\n\r\n  &__followers-grid {\r\n    @include grid(4, 2.4rem, 0);\r\n    margin-bottom: 4.8rem;\r\n  }\r\n\r\n  &__followers,\r\n  &__reach-box {\r\n    cursor: pointer;\r\n    transition: all 0.3s ease;\r\n    &:hover {\r\n      background-color: var(--clr-card-hover);\r\n    }\r\n  }\r\n\r\n  &__followers {\r\n    background-color: var(--clr-card-bg);\r\n    border-radius: 0.5rem;\r\n    padding: 1.8rem 3.6rem;\r\n    text-align: center;\r\n  }\r\n\r\n  &__facebook,\r\n  &__twitter,\r\n  &__youtube {\r\n    border-top: 4px solid;\r\n  }\r\n\r\n  &__facebook {\r\n    border-color: $fb-color;\r\n  }\r\n\r\n  &__twitter {\r\n    border-color: $twitter-color;\r\n  }\r\n\r\n  &__instagram {\r\n    position: relative;\r\n  }\r\n\r\n  &__instagram::before {\r\n    content: \"\";\r\n    position: absolute;\r\n    inset: 0;\r\n    padding-top: 0.4rem;\r\n    border-radius: 0.5rem;\r\n    background-image: linear-gradient(\r\n      to right,\r\n      hsl(37, 97%, 70%),\r\n      hsl(329, 70%, 58%)\r\n    );\r\n    -webkit-mask: linear-gradient(#fff 0 0) content-box,\r\n      linear-gradient(#fff 0 0);\r\n    -webkit-mask-composite: xor;\r\n    mask-composite: subtract;\r\n  }\r\n\r\n  &__youtube {\r\n    border-color: $youtube-color;\r\n  }\r\n\r\n  &__id-box {\r\n    @include flex(row, center, center, 0.8rem);\r\n    margin-bottom: 1.8rem;\r\n  }\r\n\r\n  &__id-name {\r\n    color: var(--text-light);\r\n    font-weight: 700;\r\n  }\r\n\r\n  &__no-of-followers {\r\n    font-size: 4.8rem;\r\n    font-weight: 700;\r\n  }\r\n\r\n  &__followers-text {\r\n    color: var(--text-light);\r\n    text-transform: uppercase;\r\n    font-size: 1.2rem;\r\n    letter-spacing: 4px;\r\n    margin-bottom: 2.4rem;\r\n  }\r\n\r\n  &__increase-box,\r\n  &__decrease-box {\r\n    @include flex(row, center, center, 0.4rem);\r\n    font-weight: 700;\r\n  }\r\n\r\n  &__increase-box {\r\n    color: $lime-green;\r\n  }\r\n\r\n  &__decrease-box {\r\n    color: $bright-red;\r\n  }\r\n\r\n  &__overview-heading {\r\n    font-size: 2.4rem;\r\n    color: var(--clr-overview-heading);\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  &__overview-grid {\r\n    @include grid(4, 2.4rem, 1.8rem);\r\n  }\r\n\r\n  &__reach-box {\r\n    background-color: var(--clr-card-bg);\r\n    border-radius: 0.5rem;\r\n    padding: 1.8rem;\r\n  }\r\n\r\n  &__reach-heading-box {\r\n    @include flex(row, space-between, center, 0);\r\n    margin-bottom: 2.4rem;\r\n  }\r\n\r\n  &__mode-of-reach {\r\n    font-size: 1.2rem;\r\n    font-weight: 700;\r\n    color: var(--text-light);\r\n  }\r\n\r\n  &__reach-description-box {\r\n    @include flex(row, space-between, center, 0);\r\n  }\r\n  &__total-reach {\r\n    font-size: 3rem;\r\n    font-weight: 700;\r\n  }\r\n}\r\n","sass\\abstract\\_mixins.scss":"@mixin flex($direction, $justify-content, $align-items, $gap) {\r\n  display: flex;\r\n  flex-direction: $direction;\r\n  justify-content: $justify-content;\r\n  align-items: $align-items;\r\n  gap: $gap;\r\n}\r\n\r\n@mixin grid($numOfCol, $col-gap, $row-gap) {\r\n  display: grid;\r\n  grid-template-columns: repeat($numOfCol, 1fr);\r\n  column-gap: $col-gap;\r\n  row-gap: $row-gap;\r\n}\r\n","sass\\abstract\\_variables.scss":"$lime-green: hsl(163, 72%, 41%);\r\n$bright-red: hsl(356, 69%, 56%);\r\n$fb-color: hsl(208, 92%, 53%);\r\n$twitter-color: hsl(203, 89%, 53%);\r\n$youtube-color: hsl(348, 97%, 39%);\r\n$toggle-color: hsl(230, 22%, 74%);\r\n"},"lineCount":null}},"error":null,"hash":"e200840c1e02a6708c14e860c144cfe0","cacheData":{"env":{}}}